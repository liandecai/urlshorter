<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.springframework.org/schema/beans" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://www.springframework.org/schema/util
       http://www.springframework.org/schema/util/spring-util-3.0.xsd">

	<util:set id="uncheckedUris" set-class="java.util.TreeSet"
		value-type="java.lang.String" scope="prototype">
        <value>/user/test.do</value>
        <value>/favicon.ico</value>
		<value>/user/login.do</value>
		<value>/index.jsp</value>
		<value>/error.jsp</value>
		<value>/403.jsp</value>
		<value>/404.jsp</value>
		<value>/500.jsp</value>
		<value>/common/error</value>

		<!-- 部门 mengfan.feng -->
		<value>/user/dep.json</value>
	</util:set>

	<util:set id="commonUris" set-class="java.util.TreeSet"
		value-type="java.lang.String" scope="prototype">
		<value>/</value>
		
		<value>/user/logout.do</value>

		<!-- 课程相关 -->
		<value>/course/scanAllCourseInMineCourse.json</value>
		<value>/course/myCourse.htm</value>
		<value>/course/scanCourseDetail.json</value>

		<!-- 留言相关接口 -->
		<value>/message/publicMessage.do</value>

		<!-- 试卷相关接口 -->
		<value>/paper/exam.htm</value>
		<value>/paper/paper.htm</value>
		<value>/paper/paper.json</value>
		<value>/paper/exam.json</value>
		<value>/paper/judge.do</value>

		<!--用户查看历史试卷成绩,详细试卷情况 -->
		<value>/paperStatistics/examHistory.json</value>
		<value>/paperStatistics/userScore.htm</value>
		<value>/paperStatistics/detailPaper.json</value>
		<value>/paperStatistics/userPaper.htm</value>
		<value>/course/scanAllCourseInMineCourse.json</value>
		<value>/message/publicMessage.do</value>
	</util:set>

	<util:set id="leaderUris" set-class="java.util.TreeSet"
		value-type="java.lang.String" scope="prototype">
		<value>/paperStatistics/leaderSearch.json</value>
		<value>/paperStatistics/scoreSubordinate.htm</value>
		<value>/course/categoryMappingCourse.json</value>
	</util:set>

	<util:set id="adminUris" set-class="java.util.TreeSet"
		value-type="java.lang.String" scope="prototype">

		<!-- 课程分类接口 -->
		<value>/category/user.json</value>

		<!-- 日志接口 -->
		<value>/log/logList.json</value>
		<value>/log/query.htm</value>

		<!-- 课程相关接口 -->
		<value>/course/touchCourse.do</value>
		<value>/course/modifyCourse.do</value>
		<value>/course/dropCourse.do</value>
		<value>/course/cancelCourse.do</value>
		<value>/course/scanAllCourseInCourseManager.json</value>
		<value>/course/categoryMappingCourse.json</value>
		<value>/course/courseList.htm</value>

		<!-- 留言相关接口 -->
		<value>/message/countMessage.json</value>
		<value>/message/scanMessageOfCourse.json</value>
		<value>/message/messageList.htm</value>

		<!-- 试卷相关接口 -->
		<value>/paper/question.json</value>
		<value>/paper/create.do</value>
		<value>/paper/create.htm</value>

		<!-- 试题相关接口 -->
		<value>/question/uploadImage.do</value>
		<value>/question//questionTemplate.xls</value>
		<value>/question/courseExam.htm</value>
		<!-- attention -->
		<value>/question/questionTemplate.xls</value>
		<value>/questionOperation/addOneQuestion.do</value>
		<value>/questionOperation/batchAddQuestion.do</value>
		<value>/questionOperation/queryQuestion.do</value>
		<value>/questionOperation/questionExport.xls</value>
		<value>/questionOperation/modifyQuestion.do</value>
		<value>/questionOperation/deleteQuestion.do</value>
		<value>/questionOperation/offlineQuestion.do</value>


		<!-- 考试结果查询相关 -->
		<value>/paperStatistics/adminSearch.json</value>
		<value>/paperStatistics/score.htm</value>
		<value>/paperStatistics/departmentStatistics.json</value>
		<value>/paperStatistics/examDetail.json</value>
		<value>/paperStatistics/export.xls</value>
		<value>/paperStatistics/paperList.json</value>
		<value>/paperStatistics/scoreDetail.htm</value>
		<value>/paperStatistics/scoreStatistics.htm</value>

		<!-- 考试人员相关 -->
		<value>/userPaper/paper.json</value>
		<value>/userPaper/paper.htm</value>
		<value>/userPaper/user.json</value>
		<value>/userPaper/user.htm</value>
		<value>/userPaper/del.do</value>
		<value>/userPaper/leave.do</value>
		<value>/userPaper/leave.json</value>
		<value>/userPaper/leave.htm</value>
		<value>/userPaper/add.do</value>

		<!-- 人员查询相关 -->
		<value>/user/userFile.json</value>
		<value>/user/user.json</value>
		<value>/user/findUserByName.json</value>

        <value>/role/superAdminDelete.do</value>
        <value>/role/superAdminAdd.do</value>

	</util:set>

	<util:set id="superAdminUris" set-class="java.util.TreeSet"
		value-type="java.lang.String" scope="prototype">
		<!-- 管理员管理相关接口 -->
		<value>/role/adminRoleList.json</value>
		<value>/role/deleUserAdmin.do</value>
		<value>/role/usersInRole.json</value>
		<value>/role/getUserAdminRoles.json</value>
		<value>/role/changeUserAdminRole.do</value>
		<value>/role/addAdminRoleUser.do</value>
		<value>/role/addAdminRole.do</value>
		<value>/role/updateAdminRole.do</value>
		<value>/role/getRoleCategoryIds.json</value>
		<value>/role/changeRoleCategory.do</value>
		<value>/role/deleteAdminRole.do</value>
		<value>/role/adminmenber.htm</value>
		<value>/role/adminrole.htm</value>
		<!-- 更新系统人员 -->
		<value>/update/memberupdate.htm</value>
		<value>/update/up.do</value>
		<!-- 免考人员设置 -->
		<value>/exemption/exemptionUserList.json</value>
		<value>/exemption/searchExemptionUser.json</value>
		<value>/exemption/removeExemptionUser.do</value>
		<value>/exemption/addExemptionUser.do</value>
		<value>/exemption/manage.htm</value>

		<!-- 课程分类接口 -->
		<value>/category/category.htm</value>
		<value>/category/one.json</value>
		<value>/category/add.do</value>
		<value>/category/mod.do</value>
		<value>/category/del.do</value>

		<!--重置考生考试成绩 -->
		<value>/paperStatistics/resetExam.do</value>

	</util:set>

	<bean id="accessRule" class="com.qunar.exam.filter.AccessRuleImpl"
		init-method="initCheckedPathMap">
		<property name="uncheckedUris" ref="uncheckedUris" />
		<property name="commonUris" ref="commonUris" />
		<property name="leaderUris" ref="leaderUris" />
		<property name="adminUris" ref="adminUris" />
		<property name="superAdminUris" ref="superAdminUris" />
	</bean>
</beans>
